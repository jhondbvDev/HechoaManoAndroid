<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MainApplication"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HechoAMano"
        tools:targetApi="31">
        <activity
            android:name=".ui.customerorders.ClientCustomerOrderActivity"
            android:exported="false" />
        <activity
            android:name=".ui.customerorders.EmptyCustomerOrderActivity"
            android:exported="false" />
        <activity
            android:name=".ui.home.LoadInventaryActivity"
            android:exported="false" />
        <activity
            android:name=".ui.stockcontrol.StockControlActivity"
            android:exported="false" />
        <activity
            android:name=".ui.productentry.ProductEntryActivity"
            android:exported="false" />
        <activity
            android:name=".ui.home.HomeActivity"
            android:exported="false" />
        <activity
            android:name=".ui.customerorders.CustomerOrderActivity"
            android:exported="false" />
        <activity
            android:name=".ui.login.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>